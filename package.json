{
  "name": "simple-screenshare",
  "version": "1.0.0",
  "description": "Turn a computer into a device to accept a screenshare.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.hello.simple-screenshare",
    "linux": {
      "target": "AppImage"
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^22.0.2",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "devcert": "^1.2.2",
    "express": "^4.18.2",
    "ip": "^1.1.8",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.6.1"
  }
}
